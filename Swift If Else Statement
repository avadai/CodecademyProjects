/*
English: EN
Spanish: ES
Chinese: ZH
Russian: RU
*/ 
var abbreviation = "RU";

if abbreviation == "EN" {
  print("English")
} else if abbreviation == "ES" {
  print("Spanish")
} else if abbreviation == "ZH" {
  print("Chinese")
} else if abbreviation == "RU" {
  print("Russian")
} else {
  print("Abbreviation not found")
}
